      window.workshops = "<%= image_url('workshops.png') %>"
      window.babyshower = "<%= image_url('babyshower.png') %>"
      window.dinners = "<%= image_url('dinners.png') %>"
      window.weddings = "<%= image_url('weddings.png') %>"
      window.teamoffsites = "<%= image_url('teamofsides.png') %>"
      window.launchparties = "<%= image_url('launchparties.png') %>"
      window.outdoorevents = "<%= image_url('outdoor.png') %>"
      window.performances = "<%= image_url('performances.png') %>"
      window.getstarted = "<%= image_url('getstarted.png') %>"


      window.specialevents = "<%= image_url('specialevents.png') %>"
      window.eventsbg = "<%= image_url('events-bg.png') %>"
      window.footerlogo = "<%= image_url('footerlogo.png') %>"

      window.hostshare = "<%= image_url('hostshare.png') %>"
      window.homes = "<%= image_url('homes.png') %>"
      window.lofts = "<%= image_url('lofts.png') %>"
      window.studios = "<%= image_url('studios.png') %>"
      window.warehouses = "<%= image_url('warehouses.png') %>"
      window.galleries = "<%= image_url('galleries.png') %>"
      window.bars = "<%= image_url('bars.png') %>"
      window.coworking = "<%= image_url('coworking.png') %>"
      window.venues = "<%= image_url('venues.png') %>"

      window.gotyourback = "<%= image_url('gotyourback.png') %>"
      window.user = "<%= image_url('user.png') %>"
      window.temporal = "<%= image_url('temporal.jpg') %>"

      // Marker SVG
      window.marker = "<%= asset_path('marker.svg') %>"
      window.star = "<%= asset_path('star.svg') %>"
      window.person = "<%= asset_path('person.svg') %>"
      window.time = "<%= asset_path('time.svg') %>"
      window.sqft = "<%= asset_path('sqft.svg') %>"
      
      window.wifi = "<%= asset_path('wifi.svg') %>"
      window.table = "<%= asset_path('table.svg') %>"
